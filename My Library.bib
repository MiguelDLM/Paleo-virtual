@article{Marce-Nogue2015,
  title = {Improving Mesh Generation in Finite Element Analysis for Functional Morphology Approaches},
  author = {{Marc{\'e}-Nogu{\'e}}, Jordi and Fortuny, Josep and Gil, Llu{\'i}s and Sanchez, Montserrat},
