@article{tomassiniTaphonomicModesFluvial2013,
  title = {Taphonomic Modes on Fluvial Deposits of the {{Monte Hermoso Formation}} (Early {{Pliocene}}), {{Buenos Aires}} Province, {{Argentina}}},
