@article{geninBiomechanicsLocomotionAsian2010,
  title = {Biomechanics of Locomotion in {{Asian}} Elephants},
  author = {Genin, J. J. and Willems, P. A. and Cavagna, G. A. and Lair, R. and Heglund, N. C.},
  year = 2010,
  month = mar,
  journal = {Journal of Experimental Biology},
  volume = {213},
  number = {5},
  pages = {694--706},
  issn = {1477-9145, 0022-0949},
  doi = {10.1242/jeb.035436},
  urldate = {2024-11-04},
  abstract = {Elephants are the biggest living terrestrial animal, weighing up to five tons and measuring up to three metres at the withers. These exceptional dimensions provide certain advantages (e.g. the mass-specific energetic cost of locomotion is decreased) but also disadvantages (e.g. forces are proportional to body volume while supportive tissue strength depends on their cross-sectional area, which makes elephants relatively more fragile than smaller animals). In order to understand better how body size affects gait mechanics the movement of the centre of mass (COM) of 34 Asian elephants (Elephas maximus) was studied over their entire speed range of 0.4--5.0ms--1 with force platforms. The mass-specific mechanical work required to maintain the movements of the COM per unit distance is \textasciitilde 0.2Jkg--1m--1 (about 1/3 of the average of other animals ranging in size from a 35g kangaroo rat to a 70kg human). At low speeds this work is reduced by a pendulum-like exchange between the kinetic and potential energies of the COM, with a maximum energy exchange of \textasciitilde 60\% at 1.4ms--1. At high speeds, elephants use a bouncing mechanism with little exchange between kinetic and potential energies of the COM, although without an aerial phase. Elephants increase speed while reducing the vertical oscillation of the COM from about 3cm to 1cm.},
  langid = {english},
  file = {/home/miguel/snap/zotero-snap/common/Zotero/storage/RL6LRBHA/Genin et al. - 2010 - Biomechanics of locomotion in Asian elephants.pdf}
}

