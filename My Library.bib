@article{somervilleRabbitBoneStable2020,
  title = {Rabbit Bone Stable Isotope Values Distinguish Desert Ecoregions of {{North America}}: {{Data}} from the Archaeological Sites of {{Pueblo Grande}}, {{La Ferreria}}, and {{La Quemada}}},
  shorttitle = {Rabbit Bone Stable Isotope Values Distinguish Desert Ecoregions of {{North America}}},
  author = {Somerville, Andrew D. and Nelson, Ben A. and Punzo D{\'i}az, Jos{\'e} Luis and Schoeninger, Margaret J.},
  year = 2020,
  month = jan,
  journal = {Journal of Archaeological Science},
  volume = {113},
  pages = {105063},
  issn = {03054403},
  doi = {10.1016/j.jas.2019.105063},
  urldate = {2024-05-16},
  abstract = {This study demonstrates the ability of light element stable isotope data from leporid (rabbit and jackrabbit) bone samples to reflect paleoenvironments at archaeological settlements in arid and semi-arid environments. A sample of 214 leporid bones recovered from the pre-Hispanic archaeological sites of Pueblo Grande (Arizona, USA), La Ferreria (Durango, Mexico) and La Quemada (Zacatecas, Mexico) are analyzed for stable isotope values of ox\- ygen ({$\delta$}18Ocarbonate) and carbon ({$\delta$}13Ccarbonate) in bone mineral carbonate, and stable isotope values of nitrogen ({$\delta$}15Ncollagen) and carbon ({$\delta$}13Ccollagen) in bone collagen. These site locations represent terrestrial ecoregions of the Sonoran Desert, the Chihuahuan Desert, and the Central Mexican Matorral, respectively. Stable isotope values are used to test predicted differences generated from known environmental parameters at these locations based on 50-year averages of modern climatic data. The resulting {$\delta$}18Ocarbonate values match expectations based on differences in mean annual precipitation; {$\delta$}15Ncollagen results confirm expectations based on relative temperature differences; and {$\delta$}13Ccarbonate and {$\delta$}13Ccollagen results support predictions based on the relative abundance of grassland environments at each location. Multivariate analyses of the four stable isotope variables together demonstrate the ability of leporid bones to distinguish between desert ecoregions and indicate the utility of including stable isotope analyses of bones from small mammals to characterize environmental conditions of past terrestrial ecoregions.},
  langid = {english},
  file = {C:\Users\Miguel\OneDrive - UT Cloud\Zotero\Somerville et al_2020_Rabbit bone stable isotope values distinguish desert ecoregions of North America.pdf}
}

@article{sorkinEcomorphologyGiantBeardogs2006,
  title = {Ecomorphology of the Giant Bear-Dogs {{{\emph{Amphicyon}}}} and {{{\emph{Ischyrocyon}}}}},
