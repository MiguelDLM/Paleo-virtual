@article{bargoBiomechanicsPalaeobiologyXenarthra,
  title = {Biomechanics and Palaeobiology of the {{Xenarthra}}: {{The}} State of the Art},
  author = {Bargo, M Susana},
  abstract = {The morphological diversity of fossil Xenarthra is striking in comparison with the living forms. They include some of the most peculiar mammals, such as the glyptodonts (Cingulata), with a globular bony carapace or the ground sloths (Tardigrada), most of them with body masses ranging from 1 to 4 tonnes. The great majority of work on the group has been related to the description of new taxa and the study of their phylogenetic relationships and stratigraphic location. But, in general, the palaeobiological aspects have not been extensively discussed or analysed. Comparison with living animals is the most common method of reconstructing modes of life of fossils. However, for many of these xenarthrans, there are no good analogues among the extant mammals, and additional tools are needed. In the last decade, several studies applying methods used in biomechanics, ecomorphology and morphometry have been undertaken to reconstruct their palaeobiology. Their results allow the development of novel palaeobiological interpretations that, coupled with palaeoenvironmental data (geology, palaeoclimatology and palaeovegetation), provide insightful information on the palaeoecological context. This paper is a review of those studies that include three main lines: 1) estimation of body masses, 2) limb bone proportions and mechanics, and 3) mechanics of the masticatory apparatus.},
  langid = {english},
  file = {C:\Users\Miguel\OneDrive - UT Cloud\Zotero\Bargo_Biomechanics and palaeobiology of the Xenarthra.pdf}
}

