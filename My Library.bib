@article{itoQuantitativeAssessmentMasticatory2024,
  title = {Quantitative Assessment of Masticatory Muscles Based on Skull Muscle Attachment Areas in {{Carnivora}}},
  author = {Ito, Kai and Kubo, Mugino O. and Kodera, Ryo and Takeda, Sei-ichiro and Endo, Hideki},
  year = 2024,
  journal = {The Anatomical Record},
  volume = {n/a},
  number = {n/a},
  issn = {1932-8494},
  doi = {10.1002/ar.25599},
  urldate = {2025-07-15},
  abstract = {Masticatory muscles are composed of the temporalis, masseter, and pterygoid muscles in mammals. Each muscle has a different origin on the skull and insertion on the mandible; thus, all masticatory muscles contract in different directions. Collecting in vivo data and directly measuring the masticatory muscles anatomically in various Carnivora species is practically problematic. This is because some carnivorans can be ferocious, rare, or even extinct. Consequently, the most practical method to collect data on the force generated by the masticatory muscle is to estimate the force based on skulls. The physiological cross-sectional area (PCSA) of each masticatory muscle, which correlates to the maximum force that can be produced by a muscle, was quantified. Using computed tomography, we defined the three-dimensional measurement area for 32 carnivoran species based on the origin and insertion of masticatory muscles specified by observable crests, ridges, and scars. Subsequent allometric analysis relating the measurement area on skull surface to the PCSA for each masticatory muscle measured in fresh specimens revealed a strong correlation between the two variables. This finding indicates that within Carnivora, an estimation of absolute masticatory muscle PCSA can be derived from measurements area on skull surface. This method allows for the use of cranial specimens, housed in museums and research institutions, that lack preserved masticatory muscles in quantitative studies involving masticatory muscle PCSA. This approach facilitates comprehensive discussions on the masticatory muscle morphology of Carnivora, including rare and extinct species.},
  langid = {english},
  keywords = {Carnivora,fascicle length,masticatory muscles,physiological cross-sectional area (PCSA),skull},
  file = {/home/miguel/snap/zotero-snap/common/Zotero/storage/IIDUI2J2/Ito et al. - Quantitative assessment of masticatory muscles based on skull muscle attachment areas in Carnivora.pdf;/home/miguel/snap/zotero-snap/common/Zotero/storage/KF2VSP8B/ito2024S1.pdf}
}

