  title = {Taxonomy, Diet and Geographical Provenance of Fossil Species of {{Sigmodon}} from West Central {{Mexico}} during the Late {{Pleistocene}}},
  author = {{Cervantes-Barriga}, Raymundo and {Mar{\'i}n-Leyva}, Alejandro Hiram and {Garc{\'i}a-Zepeda}, Mar{\'i}a Luisa and {Ponce-Saavedra}, Javier and {Arroyo-Cabrales}, Joaqu{\'i}n and Schaaf, Peter and {Sol{\'i}s-Pichardo}, Gabriela and {Monterrubio-Rico}, Tiberio and {Cano-Camacho}, Horacio},
  year = 2021,
