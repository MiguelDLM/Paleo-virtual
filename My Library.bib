  author = {Coltrain, Joan Brenner and Harris, John M. and Cerling, Thure E. and Ehleringer, James R. and Dearing, Maria-Denise and Ward, Joy and Allen, Julie},
  year = 2004,
  month = mar,
  journal = {Palaeogeography, Palaeoclimatology, Palaeoecology},
  volume = {205},
