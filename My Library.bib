@article{perez-crespoDietHabitatMesomammals2018,
  title = {Diet and Habitat of Mesomammals and Megamammals from {{Cedral}}, {{San Luis Potos\'i}}, {{M\'exico}}},
  author = {{P{\'e}rez-Crespo}, V{\'i}ctor Adri{\'a}n and {Arroyo-Cabrales}, Joaqu{\'i}n and {Morales-Puente}, Pedro and {Cienfuegos-Alvarado}, Edith and Otero, Francisco J.},
