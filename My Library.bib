@article{schwartzInvestigatingPreHispanicScarlet2021,
  title = {Investigating Pre-{{Hispanic}} Scarlet Macaw Origins through Radiogenic Strontium Isotope Analysis at {{Paquim\'e}} in {{Chihuahua}}, {{Mexico}}},
