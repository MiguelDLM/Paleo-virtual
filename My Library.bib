@article{rinconNewEarlyMiocene2015,
  title = {New Early {{Miocene}} Protoceratids ({{Mammalia}}, {{Artiodactyla}}) from {{Panama}}},
  author = {Rincon, Aldo F. and Bloch, Jonathan I. and Macfadden, Bruce J. and Jaramillo, Carlos A.},
