  author = {Angles, Baptiste and Rebain, Daniel and Macklin, Miles and Wyvill, Brian and Barthe, Loic and Lewis, Jp and Von Der Pahlen, Javier and Izadi, Shahram and Valentin, Julien and Bouaziz, Sofien and Tagliasacchi, Andrea},
