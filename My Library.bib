@article{alvarez-carreteroBayesianEstimationSpecies2019,
  title = {Bayesian {{Estimation}} of {{Species Divergence Times Using Correlated Quantitative Characters}}},
  author = {{\'A}lvarez-Carretero, Sandra and Goswami, Anjali and Yang, Ziheng and Dos Reis, Mario},
