@article{macfaddenNorthAmericanMiocene2006,
  title = {North {{American Miocene}} Land Mammals from {{Panama}}},
  author = {Macfadden, Bruce J.},
  year = 2006,
  month = sep,
  journal = {Journal of Vertebrate Paleontology},
  volume = {26},
