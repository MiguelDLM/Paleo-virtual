@article{perez-ramosBiomechanicalSimulationsReveal2020,
  title = {Biomechanical Simulations Reveal a Trade-off between Adaptation to Glacial Climate and Dietary Niche Versatility in {{European}} Cave Bears},
  author = {{P{\'e}rez-Ramos}, Alejandro and Tseng, Z. Jack and {Grandal-D'Anglade}, Aurora and Rabeder, Gernot and Pastor, Francisco J. and Figueirido, Borja},
