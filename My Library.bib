@article{arbourAnalyzingTaphonomicDeformation2012,
  title = {Analyzing {{Taphonomic Deformation}} of {{Ankylosaur Skulls Using Retrodeformation}} and {{Finite Element Analysis}}},
  author = {Arbour, Victoria M. and Currie, Philip J.},
  editor = {Dodson, Peter},
  year = 2012,
