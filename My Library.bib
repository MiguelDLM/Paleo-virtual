@article{adrianperez-crespoFeedingHabitsMobility2022,
  title = {Feeding Habits and Mobility of {{{\emph{Equus}}}}{\emph{ Conversidens}} and {{{\emph{E}}}}{\emph{. Mexicanus}} from {{La Presita Blanca}}, {{State}} of {{San Luis Potos\'i}}, {{M\'exico}}},
  author = {{Adri{\'a}n P{\'e}rez-Crespo}, V{\'i}ctor and {Cienfuegos-Alvarado}, Edith and Schaaf, Peter and {Sol{\'i}s-Pichardo}, Gabriela and J. Otero, Francisco and {Torres-Hern{\'a}ndez}, Jos{\'e} Ram{\'o}n},
  year = 2022,
  month = feb,
  journal = {Historical Biology},
  volume = {34},
  number = {2},
  pages = {216--225},
  issn = {0891-2963, 1029-2381},
  doi = {10.1080/08912963.2021.1909008},
  urldate = {2024-05-16},
  abstract = {The aim of this study is to determine the feeding habits and home range of fossil horses (Equus conversidens and E. mexicanus) from La Presita Blanca, San Luis Potos\'i, central M\'exico. For this purpose, we used carbon, oxygen, and strontium isotope ratios. We compared 87Sr/86Sr from horses with strontium ratios of La Presita Blanca plants and soils to identify if these animals were local or came from elsewhere. The {$\delta$}13C values indicate that E. conversidens specimen consumed C4 plants and E. mexicanus was a mixed feeder. In this case, carbon isotopic values reflect only the animal's feeding preferences and not the type of vegetation that existed in La Presita Blanca during the Pleistocene. Furthermore, {$\delta$}18O values and strontium isotopic ratios show that E. conversidens was an animal that could be local to the Campo Volc\'anico de San Luis Potos\'i whereas E. mexicanus came from another region.},
  langid = {english},
  file = {C:\Users\Miguel\OneDrive - UT Cloud\Zotero\Adrián Pérez-Crespo et al_2022_Feeding habits and mobility of iEquus conversidens-i and iE.pdf}
}

