@article{diaz-sibajaCombinedMesowearAnalysis2018,
  title = {A Combined Mesowear Analysis of {{Mexican}} {{{\emph{Bison}}}}{\emph{ Antiquus}} Shows a Generalist Diet with Geographical Variation},
  author = {{D{\'i}az-Sibaja}, Roberto and {Jim{\'e}nez-Hidalgo}, Eduardo and {Ponce-Saavedra}, Javier and {Garc{\'i}a-Zepeda}, Mar{\'i}a Luisa},
