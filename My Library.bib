@incollection{simmonsPhanerozoicEustasy2020,
  title = {Phanerozoic {{Eustasy}}},
  booktitle = {Geologic {{Time Scale}} 2020},
  author = {Simmons, M.D. and Miller, K.G. and Ray, D.C. and Davies, A. and Van Buchem, F.S.P. and Gr{\'e}selle, B.},
  year = 2020,
  pages = {357--400},
  publisher = {Elsevier},
  doi = {10.1016/B978-0-12-824360-2.00013-9},
  urldate = {2025-10-20},
  abstract = {Isolation of the eustatic signal from the sedimentary record remains challenging, yet much progress is being made toward understanding the timing, magnitude, and rate of eustasy on both long-term (107 108 years) and short-term (105 106 years) scales throughout the Phanerozoic. Long-term eustasy is primarily driven by a number of factors relating to plate tectonics. The magnitude and rate of shortterm eustatic change strongly suggests glacio-eustasy as the key driving mechanism, even in episodes of the Earth's history often typified as having ``greenhouse'' climates. This notion is, in turn, supported by a growing body of both direct and proxy evidence for relatively substantial polar glaciation in many periods of the Earth's history (with the possible exception of the Triassic). An understanding of eustasy is important for the development of the geologic time scale because it contributes to the sequence stratigraphic organization of sedimentary successions (including chronostratigraphic reference sections) and helps to understand the often incomplete nature of the geologic record. The integration of eustasy with our evolving knowledge of the Earth systems science (e.g., paleoclimate evolution, orbital forcing of sedimentary systems, geochemical evolution of the oceans, and biological evolutions and extinctions) will help to provide the tools to develop a context for the subdivisions of the geologic time scale.},
  copyright = {https://www.elsevier.com/tdm/userlicense/1.0/},
  isbn = {978-0-12-824360-2},
  langid = {english},
  keywords = {/unread},
  file = {/home/miguel/snap/zotero-snap/common/Zotero/storage/QRHQA5YB/Simmons et al. - 2020 - Phanerozoic Eustasy.pdf}
}

