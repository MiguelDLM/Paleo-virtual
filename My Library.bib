  author = {{P{\'e}rez-Crespo}, V{\'i}ctor Adri{\'a}n and {Carranza-Casta{\~n}eda}, Oscar and {Arroyo-Cabrales}, Joaqu{\'i}n and {Morales-Puente}, Pedro and {Cienfuegos-Alvarado}, Edith and Otero, Francisco J.},
  year = 2017,
