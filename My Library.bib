}

@article{sosaMAYACOASTALPRODUCTION2014,
  title = {{{MAYA COASTAL PRODUCTION}}, {{EXCHANGE}}, {{LIFE STYLE}}, {{AND POPULATION MOBILITY}}: {{A VIEW FROM THE PORT OF XCAMBO}}, {{YUCATAN}}, {{MEXICO}}},
  shorttitle = {{{MAYA COASTAL PRODUCTION}}, {{EXCHANGE}}, {{LIFE STYLE}}, {{AND POPULATION MOBILITY}}},
